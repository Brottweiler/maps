<spawns>
    <spawn team="blue" kit="blue" yaw="180">
        <cuboid min="-681,51,-11" max="-677,51,-8"/>
    </spawn>
    <spawn team="red" kit="red" yaw="0">
        <cuboid min="-667,51,-340" max="-663,51,-337"/>
    </spawn>
    <default yaw="90">
        <cylinder base="-782.5,119,-174.5" radius="3" height="0"/>
    </default>
</spawns>
<regions>
    <apply block="deny-all">
        <rectangle min="-oo,-oo" max="oo,oo"/>
    </apply>
</regions>
<killreward>
    <item damage="8229">potion</item> <!-- Potion of Health 2 -->
</killreward>
<score>
    <time>10m</time>
</score>
<toolrepair>
    <tool>bow</tool>
    <tool>stone sword</tool>
</toolrepair>
<itemremove>
    <item>arrow</item>
    <item>leather helmet</item>
    <item>leather chestplate</item>
    <item>leather leggings</item>
    <item>leather boots</item>
