<?xml version="1.0"?>
<map proto="1.3.0">
<name>Two Castles</name>
<version>2.0</version>
<objective>Take the enemy's flag located behind the enemy's base and capture it at your victory monument.</objective>
<authors>
    <author uuid="e5953ddf-c1fc-4405-9ac9-6939631cd185"/> <!-- McSpider -->
    <author uuid="3ce426cd-2ded-4f83-a9bb-46a57d68d9a5"/> <!-- McYukon -->
</authors>
<contributors>
    <contributor uuid="6968cbde-c2b7-479c-a8da-f33f109f013b" contribution="Capture the Flag version"/> <!-- Brottweiler -->
</contributors>
<score>
    <limit>1</limit>
</score>
<teams>
    <team id="red-team" color="dark red" max="20">Red</team>
    <team id="blue-team" color="blue" max="20">Blue</team>
</teams>
<flags>
    <flag id="blue-flag" name="Blue Flag" owner="blue-team">
        <post pickup-filter="only-red">0,11,93</post>
    </flag>

    <flag id="red-flag" name="Red Flag" owner="red-team">
        <post pickup-filter="only-blue">0,11,-92</post>
    </flag>
    <flags points="1">
        <net region="red-net" flag="blue-flag" rescue="red-flag"/>
        <net region="blue-net" flag="red-flag" rescue="blue-flag"/>
    </flags>
</flags>
<kits>
    <kit name="spawn">
        <item slot="0">iron sword</item>
        <item slot="1" enchantment="arrow infinite:1">bow</item>
        <item slot="28" amount="1">arrow</item>
        <item slot="2" enchantment="dig speed:1">iron pickaxe</item>
        <item slot="3" enchantment="dig speed:1">iron axe</item>
        <item slot="5" amount="16">glass</item>
        <item slot="6" amount="64">log</item>
        <item slot="8" amount="64">apple</item>
        <potion duration="10" amplifer="4">damage resistance</potion>
    </kit>
    <kit name="red" parents="spawn">
        <helmet color="cd0000">leather helmet</helmet>
        <chestplate enchantment="durability:1">iron chestplate</chestplate>
        <leggings color="cd0000">leather leggings</leggings>
        <boots enchantment="protection fall:1">iron boots</boots>
    </kit>
    <kit name="blue" parents="spawn">
        <helmet color="0066cc">leather helmet</helmet>
        <chestplate enchantment="durability:1">iron chestplate</chestplate>
        <leggings color="0066cc">leather leggings</leggings>
        <boots enchantment="protection fall:1">iron boots</boots>
    </kit>
    <kit name="blue-net" force="true">
        <helmet damage="3">wool</helmet>
    </kit>
    <kit name="red-net" force="true">
        <helmet damage="6">wool</helmet>
    </kit>
</kits>
<spawns>
    <spawn team="red" kit="red" yaw="90" safe="true">
        <cuboid min="-2,14,71" max="-9,14,78"/>
    </spawn>
    <spawn team="blue" kit="blue" yaw="-90" safe="true">
        <cuboid min="2,14,-71" max="9,14,-78"/>
    </spawn>
    <default yaw="0">
        <cuboid min="-1,16,-1" max="2,16,2"/>
    </default>
</spawns>
<filters>
    <filter name="only-red">
        <team>red</team>
    </filter>
    <filter name="only-blue">
        <team>blue</team>
    </filter>
</filters>
<regions>
    <!-- Blue Team Base -->
    <apply enter="only-blue" message="You may not enter the enemy team's base!">
        <cuboid min="-20,8,-75" max="11,41,-62"/>
        <cuboid min="8,8,-80" max="13,41,-75"/>
        <cuboid min="-12,8,-75" max="11,41,-79"/>
    </apply>
    <apply block="deny-all">
        <cuboid min="-24,8,-58" max="24,40,-100"/>
    </apply>
    <!-- Blue Team Tree -->
    <apply block="deny-all" message="This tree is protected, go figure.">
        <circle center="83.5,-47.5" radius="4"/>
    </apply>
    <!-- Blue Team Flag -->
    <cylinder name="blue-net" base="16.5,11,-92.5" radius="3" height="3"/>
    <!-- Red Team Base -->
    <apply enter="only-red" message="You may not enter the enemy team's base!">
        <cuboid min="20,8,75" max="-10,40,63"/>
        <cuboid min="-8,8,80" max="-12,40,76"/>
        <cuboid min="12,8,75" max="-11,41,79"/>
    </apply>
    <apply block="deny-all">
        <cuboid min="24,8,58" max="-24,40,100"/>
    </apply>
    <!-- Red Team Tree -->
    <apply block="deny-all" message="This tree is protected, go figure.">
        <circle center="-82.5,48.5" radius="4"/>
    </apply>
    <!-- Red Team Flag -->
    <cylinder name="red-net" base="-15.5,11,93.5" radius="3" height="3"/>
    <!--- Main Playing Area -->
    <apply block="deny-all" message="You may not build outside the playing area.">
        <negative name="playable-inverse">
            <rectangle min="101,101" max="-100,-100"/>
        </negative>
    </apply>
</regions>
<toolrepair>
    <tool>iron sword</tool>
    <tool>bow</tool>
    <tool>iron pickaxe</tool>
    <tool>iron axe</tool>
</toolrepair>
<itemremove>
    <item>glass</item>
    <item>log</item>
    <item>wood</item>
    <item>leather helmet</item>
    <item>iron chestplate</item>
    <item>leather leggings</item>
    <item>arrow</item>
    <item>iron boots</item>
    <item>seeds</item>
    <item>sapling</item>
    <item>apple</item>
    <item>yellow flower</item>
    <item>red rose</item>
    <item>wool</item>
</itemremove>
<maxbuildheight>40</maxbuildheight>
</map>
